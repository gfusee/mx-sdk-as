{
  "targets": {
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 2,
      "converge": true,
      "noAssert": false
    }
  },
  "options": {
    "runtime": "minimal",
    "enable": ["gc", "reference-types"],
    "disable": ["bulk-memory"],
    "use": "abort=wasiabort",
    "transform": ["./bindgen/dist/transformer.js"]
  }
}
